{
  "name": "paint-gauge",
  "version": "1.0.0",
  "description": "MERN stack web app for rating paintings from the Harvard Art Museum",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "node src/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "heroku-postbuild": "npm run build",
    "postman": "cross-env NODE_ENV=testing nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "check-licenses": "npx license-checker --onlyAllow \"Apache-2.0;BSD;CC0-1.0;CC-BY-3.0;CC-BY-4.0;ISC;MIT;WTFPL;UNLICENSE;Zlib;Apache License, Version 2.0\" --summary",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "James DiGrazia",
  "license": "Apache 2.0",
  "dependencies": {
    "@material-ui/core": "3.9.1",
    "@material-ui/icons": "3.0.2",
    "@material-ui/lab": "3.0.0-alpha.30",
    "autoprefixer": "9.4.7",
    "babel-polyfill": "^6.26.0",
    "body-parser": "1.18.3",
    "compression": "1.7.3",
    "connect-redis": "3.4.0",
    "cookie-parser": "1.4.3",
    "csurf": "1.8.3",
    "dotenv": "6.2.0",
    "express": "^4.16.3",
    "express-session": "1.15.6",
    "jquery": "3.3.1",
    "mongoose": "5.4.7",
    "path": "0.12.7",
    "prop-types": "15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router-dom": "4.3.1",
    "request": "2.88.0",
    "typeface-roboto": "0.0.54",
    "webpack": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.0",
    "babel-jest": "24.1.0",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.0.0",
    "cross-env": "5.2.0",
